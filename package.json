{
  "name": "ai-al-gaib",
  "version": "0.1.0",
  "description": "Multi-Agent AI Orchestration System",
  "main": "dist/cli/index.js",
  "bin": {
    "ai-al-gaib": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "electron": "electron dist/electron/main.js",
    "ui:dev": "cd ui && npm run dev",
    "app:dev": "tsc && concurrently \"npm run ui:dev\" \"cross-env NODE_ENV=development wait-on http://localhost:5173 && npm run electron\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.14",
    "marked": "^12.0.0",
    "ora": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "eslint": "^8.56.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "wait-on": "^9.0.3"
  },
  "type": "module"
}
